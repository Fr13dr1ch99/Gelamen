{
	"STRING": {
		"regex": "(.*)",
		"database": "TEXT"
	},
	"INT": {
		"regex": "(^([+-]?[1-9]\\d*|0)$)",
		"database": "INTEGER"
	},
	"FLOAT": {
		"regex": "(^[+-]?([0-9]+([.][0-9]*)?|[.][0-9]+)$)",
		"database": "FLOAT"
	},
	"BOOLEAN": {
		"regex": "(^true$|^false$)",
		"database": "BOOLEAN"
	},
	"EMAIL": {
		"regex": "^(([^<>()\\[\\]\\\\.,;:\\s@']+(\\.[^<>()\\[\\]\\\\.,;:\\s@']+)*)|('.+'))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$",
		"database": "TEXT"
	},
	"HHMM": {
		"regex": "(^(2[0-3]|[01]?[0-9]):([0-5]?[0-9])$)",
		"database": "TEXT"
	},
	"HHMMSS": {
		"regex": "(^(2[0-3]|[01]?[0-9]):([0-5]?[0-9]):([0-5]?[0-9])$)",
		"database": "TEXT"
	},
	"OBJECT": "OBJECT"
}
